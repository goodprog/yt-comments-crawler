{
    "manifest_version": 3,
    "name": "YouTube Comments Crawler",
    "description": "Extracts all comments from the YouTube video page, sorts them by the amount of likes and saves them to a csv file.",
    "version": "0.2",
    "icons": {
        "16": "images/ico16.png",
        "19": "images/ico19.png",
        "38": "images/ico38.png",
        "48": "images/ico48.png",
        "128": "images/ico128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://www.youtube.com/*"
            ],
            "js": [
                "content_script.js"
            ]
        }
    ]
}